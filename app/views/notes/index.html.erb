<p id="notice"><%= notice %></p>

<h1>Notes</h1>



<table class="table table-striped table-bordered table-hover" id="note">
  <thead>
    <tr>
      <th>Description</th>
      <th>Company</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @notes %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_company_note_path(@company),id: 'new_link',  remote: true, 'data-toggle': "modal",'data-target': "#myModal" %>
